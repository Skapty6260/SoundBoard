{
	"name": "soundboard",
	"private": true,
	"description": "Free open-source, cross-platform soundpad alternative for Windows, macOS and Linux. With internal free sound workshop and editor",
	"version": "0.0.0",
	"author": "Skapty6260",
	"type": "module",
	"engines": {
		"node": ">=16.0.0",
		"npm": "please-use-yarn",
		"yarn": "^1.22.19"
	},
	"scripts": {
		"dev": "vite",
		"build:windows": "tsc && vite build && electron-builder --win",
		"build:linux": "tsc && vite build && electron-builder --linux",
		"build:mac": "tsc && vite build && electron-builder --mac",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview"
	},
	"dependencies": {
		"ambisonics": "^0.4.0",
		"autoprefixer": "^10.4.19",
		"electron-store": "^10.0.0",
		"framer-motion": "^11.2.13",
		"fs-extra": "^11.2.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^5.2.1",
		"react-router": "^6.24.1",
		"react-router-dom": "^6.24.1"
	},
	"devDependencies": {
		"@types/react": "^18.2.64",
		"@types/react-dom": "^18.2.21",
		"@typescript-eslint/eslint-plugin": "^7.1.1",
		"@typescript-eslint/parser": "^7.1.1",
		"@vitejs/plugin-react": "^4.2.1",
		"electron": "^30.0.1",
		"electron-builder": "^24.13.3",
		"electron-updater": "^6.2.1",
		"eslint": "^8.57.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"postcss": "^8.4.40",
		"sass": "^1.77.8",
		"tailwindcss": "^3.4.7",
		"typescript": "^5.2.2",
		"vite": "^5.1.6",
		"vite-plugin-electron": "^0.28.6",
		"vite-plugin-electron-renderer": "^0.14.5",
		"vite-tsconfig-paths": "^4.3.2"
	},
	"main": "dist-electron/main.js"
